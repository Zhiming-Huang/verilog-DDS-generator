FUNCTION sign_div_unsign_dkh (denominator[6..0], numerator[6..0])
RETURNS ( quotient[6..0], remainder[6..0]);

--synthesis_resources = lut 35 
SUBDESIGN lpm_divide_72m
( 
	denom[6..0]	:	input;
	numer[3..0]	:	input;
	quotient[3..0]	:	output;
	remain[6..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_dkh;
	gnd_wire	: WIRE;
	num_padder[2..0]	: WIRE;
	numer_tmp[6..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	gnd_wire = B"0";
	num_padder[] = ( gnd_wire, gnd_wire, gnd_wire);
	numer_tmp[] = ( num_padder[], numer[]);
	quotient[3..0] = divider.quotient[3..0];
	remain[] = divider.remainder[];
END;
